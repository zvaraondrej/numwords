{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": [0],
        "import/no-extraneous-dependencies": [2, { "devDependencies": true }]
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "commonjs": true
    }
}